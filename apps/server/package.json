    "mongodb-memory-server": "8.12.2",
    "ts-node-dev": "2.0.0",
    "tsup": "6.6.3",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@swc/core": "1.3.36",
    "@swc/helpers": "0.4.14",
